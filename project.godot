config_version=5

[application]

config/name="Crystaleum"
run/main_scene="res://scenes/crys.tscn"
config/features=PackedStringArray("4.1", "Mobile")
config/icon="res://icon.svg"

[autoload]

DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"

[display]

window/size/viewport_width=320
window/size/viewport_height=180
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

[internationalization]

locale/translations_pot_files=PackedStringArray("res://dialogos/DialogoExploradora.dialogue", "res://dialogos/DialogoGuardiao.dialogue", "res://dialogos/DialogoMercenaria.dialogue", "res://dialogos/DialogoCa√ßador.dialogue")

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="mobile"
